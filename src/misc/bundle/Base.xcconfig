TM_CXXAXEL = 
TM_CXXCAIRO = 
TM_CXXIMLIB2 = 
TM_CXXFREETYPE = -I/usr/include -Ic:/TmBuildEnv/MSYS/include/freetype2 -Ic:/TmBuildEnv/MSYS/include
