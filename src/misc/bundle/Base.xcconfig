TM_LDGUILE = -I/c/TmBuildEnv/MinGW/include -I/c/TmBuildEnv/MSYS/include  -Lc:/TmBuildEnv/MSYS/lib -lguile -lltdl -L/c/TmBuildEnv/MinGW/lib -L/c/TmBuildEnv/MSYS/lib -lgmp -lws2_32 -lm -lltdl

